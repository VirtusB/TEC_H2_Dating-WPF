    <Grid Background="#333">
        <Label HorizontalAlignment="Center" Foreground="White" Content="Dashboard"></Label>
        <Label HorizontalAlignment="Center" Margin="0 20 0 0" x:Name="lblPublicUsername" Foreground="White" Content="Username"></Label>
    </Grid>
    
    
</Window>
