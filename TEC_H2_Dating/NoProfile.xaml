<Window x:Class="TEC_H2_Dating.NoProfile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TEC_H2_Dating"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Ingen profil" Height="300" Width="300">

    <!--Dete er et window-->

    <Grid Background="#333">
        <StackPanel>
            <Label FontSize="18" Foreground="IndianRed" HorizontalAlignment="Center" Content="Du har ingen profil"></Label>
            <Button Click="noProfileBtn_Click" x:Name="noProfileBtn" Margin="50 20 50 0" Padding="0 5 0 5" Background="#545d6a" Foreground="White" Content="Opret Profil"></Button>
        </StackPanel>
        
    </Grid>
    
</Window>
